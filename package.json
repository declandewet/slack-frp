{
  "name": "slock",
  "description": "promise-based and functional reactive bindings to Slack's API",
  "version": "0.0.7",
  "keywords": [
    "slack",
    "api",
    "functional",
    "reactive",
    "rtm",
    "real",
    "time",
    "web",
    "hook",
    "bot",
    "integration",
    "incoming",
    "outgoing",
    "promise"
  ],
  "scripts": {
    "release": "npm publish",
    "prerelease": "mv lib src && babel src -d lib --optional runtime --stage 0",
    "postrelease": "rm -rf lib && mv src lib",
    "test": "mocha",
    "coverage": "babel-node ./node_modules/.bin/isparta cover _mocha",
    "precoveralls": "npm run coverage",
    "coveralls": "coveralls < coverage/lcov.info"
  },
  "main": "lib",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/declandewet/slock"
  },
  "bugs": {
    "url": "http://github.com/declandewet/slock/issues",
    "email": "declandewet@me.com"
  },
  "directories": {
    "lib": "lib"
  },
  "devDependencies": {
    "babel": "^6.5.2",
    "chai": "^3.2.0",
    "chai-as-promised": "^6.0.0",
    "coveralls": "^2.11.4",
    "isparta": "^4.0.0",
    "mocha": "^3.1.2",
    "mock-socket": "^6.0.1",
    "nock": "^9.0.2"
  },
  "dependencies": {
    "babel-runtime": "^6.18.0",
    "baconjs": "^0.7.71",
    "node-fetch": "^1.3.2",
    "ws": "^1.1.1"
  }
}
