{
  "name": "slack-frp",
  "description": "a terse, functional reactive slack api",
  "version": "0.0.0",
  "keywords": [
    "slack",
    "api",
    "functional",
    "rtm",
    "real",
    "time",
    "web",
    "hook",
    "bot",
    "integration"
  ],
  "scripts": {
    "prepublish": "mv lib src && babel src -d lib --optional runtime --stage 0",
    "postpublish": "rm -rf lib && mv src lib",
    "test": "babel-node node_modules/.bin/isparta cover node_modules/.bin/_mocha"
  },
  "main": "lib",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/declandewet/slack-frp"
  },
  "bugs": {
    "url": "http://github.com/declandewet/slack-frp/issues",
    "email": "declandewet@me.com"
  },
  "directories": {
    "lib": "lib"
  },
  "devDependencies": {
    "babel": "^5.8.23",
    "chai": "^3.2.0",
    "isparta": "^3.0.4",
    "istanbul": "^0.3.19",
    "mocha": "^2.3.0"
  },
  "dependencies": {
    "baconjs": "^0.7.71",
    "node-fetch": "^1.3.2",
    "ws": "^0.8.0"
  }
}
